using System;

class Program {
    static void Main() {
        Console.Write("Minutos consumidos: ");
        int min = int.Parse(Console.ReadLine());

        double valor = 50.0;

        if (min > 100) {
            valor += (min - 100) * 2.0;
        }

        Console.WriteLine("Valor a pagar: R$ " + valor.ToString("F2"));
    }
}
